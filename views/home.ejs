<%- include('partials/header') -%> 

    <h1>HOME</h1>
    <p> <%= homeStartingContent %> </p>
    
    <h2>Posts</h2>

    <% if(posts.length > 0){ %>
        <% posts.forEach(function(post) { %>
            <h3><%= post.title %></h3>
            <p><%= post.text %>  <a href="posts/<%= post.slug %>">read more</a> </p> 
         <% }); %> 
      <% } else{ %>  
        <p class="p-empty">No hay posts</p>
    <% } %>

    
<%- include('partials/footer') -%>

